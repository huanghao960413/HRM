<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hh.dao.PositionDao">
    <insert id="addPosition"></insert>
    <update id="updatePosition"></update>
    <delete id="delPosition"></delete>
    <select id="queryPosition" resultType="Position">
        SELECT * FROM T_POSITION WHERE P_ID=#{p_id}
    </select>
    <select id="queryPositionList" resultType="Position">
        SELECT * FROM T_POSITION WHERE D_ID=#{d_id}
    </select>
    <select id="queryPositionLimit" resultType="Position"></select>
</mapper>